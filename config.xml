<?xml version='1.0' encoding='utf-8'?>
<widget id="com.citiger.residente" version="2.1.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>Citiger</name>
    <description>
        Citiger para residentes.
    </description>
    <author email="riv.edu10@gmail.com" href="https://github.com/eduardxlr">
        CitigerProject Team
    </author>
    <icon src="/www/img/citigerLogo.png" platform="android" width="57" height="57" density="mdpi" />
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <allow-intent href="market:*" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
    </platform>
</widget>
