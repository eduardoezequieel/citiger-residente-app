<?xml version='1.0' encoding='utf-8'?>
<widget id="com.citiger.residente" version="2.1.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <feature name="Whitelist">
        <param name="android-package" value="org.apache.cordova.whitelist.WhitelistPlugin" />
        <param name="onload" value="true" />
    </feature>
    <name>Citiger</name>
    <description>
        Citiger para residentes.
    </description>
    <author email="riv.edu10@gmail.com" href="https://github.com/eduardxlr">
        CitigerProject Team
    </author>
    <icon density="mdpi" height="57" platform="android" src="/www/img/citigerLogo.png" width="57" />
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <allow-intent href="market:*" />
    <preference name="loglevel" value="DEBUG" />
</widget>
